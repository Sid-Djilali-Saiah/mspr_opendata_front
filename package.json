{
  "name": "mspr-opendata-front",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --detectOpenHandles",
    "test:ci": "ng test --ci",
    "test:update": "ng test -u",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "format": "npx pretty-quick --staged",
    "sonar": "npx sonar-scanner",
    "e2e": "ng e2e",
    "start:compodoc": "npx compodoc -p ./tsconfig.json -r 4201 -w -s -o --language fr-FR"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/material": "^11.2.1",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@ngneat/until-destroy": "^8.0.4",
    "angular-file-uploader": "^7.0.3",
    "angular-material-fileupload": "^3.0.2",
    "mat-file-upload": "^11.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "husky": "^4.3.8",
    "intersection-observer": "^0.12.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-preset-angular": "^8.3.2",
    "jest-sonar": "^0.2.12",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.9",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint",
      "pre-push": "npm run test && npm run build"
    }
  },
  "engines": {
    "node": "~14"
  }
}
